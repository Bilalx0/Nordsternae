{
  "version": 2,
  "framework": "vite",
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/articles/(.*)",
      "destination": "/api/articles/$1"
    },
    {
      "source": "/api/banner-highlights/(.*)",
      "destination": "/api/banner-highlights/$1"
    },
    {
      "source": "/api/developers/(.*)",
      "destination": "/api/developers/$1"
    },
    {
      "source": "/api/developments/(.*)",
      "destination": "/api/developments/$1"
    },
    {
      "source": "/api/enquiries/(.*)",
      "destination": "/api/enquiries/$1"
    },
    {
      "source": "/api/neighborhoods/(.*)",
      "destination": "/api/neighborhoods/$1"
    },
    {
      "source": "/api/sitemap/(.*)",
      "destination": "/api/sitemap/$1"
    },
    {
      "source": "/api/properties/(.*)",
      "destination": "/api/properties/$1"
    },
    {
      "source": "/api/import-properties",
      "destination": "/api/import-properties"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "/client/dist/assets/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "/client/dist/favicon.ico"
    },
    {
      "source": "/(.*)",
      "destination": "/client/dist/index.html"
    }
  ]
}
